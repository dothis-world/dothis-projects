{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false
    },
    "request-web#build": {
      "dependsOn": [
        "^request-web#db-generate",
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "request-web#dev": {
      "dependsOn": [
        "^theme-gen",
        "^request-web#db-generate"
      ],
      "cache": false
    },
    "build-storybook": {
      "inputs": [
        ".storybook/**/*",
        "src/components/**/*.stories.tsx"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "theme-gen": {
      "inputs": [
        "./src/styles/**/*"
      ],
      "outputs": [
        "./src/types/theme.d.ts"
      ],
      "cache": false
    },
    "request-web#db-generate": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ],
      "cache": false
    },
    "request-web#db-push": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ],
      "cache": false
    },
    "studio": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ]
    },
    "analyze": {
      "inputs": [
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev-mock": {
      "cache": false
    },
    "build:theme": {

        "outputs": [
            "./dist/**/*"
        ],
        "cache": true
    },
    "storybook": {
      "dependsOn": [
        "^build:theme"
      ],
      "cache": false
    },
    "request-web": {
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "start:dev-api": {
      "cache": false,
      "outputs": [
        "dist/**/*"
      ]
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY",
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".env",
    ".env.local",
    "tsconfig.json"
  ]
}
