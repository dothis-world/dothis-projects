{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^db-generate",
        "^theme-generate",
        "^build"
      ],
      "outputs": [
        ".next/**" 
      ]
    },
    "theme-generate": {
      "inputs": [
        "./lib/styles/**/*"
      ],
      "outputs": [
        "./lib/types/theme.d.ts"
      ],
      "cache": true
    },
    "db-generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": true
    },
    "db-push": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "dev:request": {
      "dependsOn": [
        "^dev:share"
      ]
    },
    "dev:dashboard": {
      "dependsOn": [
        "^dev:share"
      ]
    },
    "dev:share": {
      "dependsOn": [
        "^theme-generate"
      ]
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY"
  ]
}
