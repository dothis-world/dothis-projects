{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false
    },
    "request-web#build": {
      "dependsOn": [
        "^request-web#db-generate",
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "request-web#dev": {
      "dependsOn": [
        "^theme-gen",
        "^request-web#db-generate"
      ],
      "cache": false
    },
    "build-storybook": {
      "inputs": [
        "src/components/**/*.stories.tsx"
      ],
      "outputs": [
        "storybook-static/**" 
      ]
    },
    "theme-gen": {
      "inputs": [
        "./src/styles/**/*"
      ],
      "outputs": [
        "./src/types/theme.d.ts"
      ],
      "cache": false
    },
    "request-web#db-generate": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ],
      "cache": false
    },
    "request-web#db-push": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ],
      "cache": false
    },
    "studio": {
      "inputs": [
        "/apps/request-web/prisma/schema.prisma"
      ]
    },
    "analyze": {
      "inputs": [
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
   
    "dev-mock": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "request-web": {
      "dependsOn": [
        "^build"
      ],
      "env": [
        "DATABASE_URL",
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET",
        "DOTHIS_AWS_ACCESS_KEY_ID",
        "DOTHIS_AWS_SECRET_ACCESS_KEY"
      ]
    },
    "clean": {
      "cache": false
    },
    "start:dev": {
      "cache": false,
      "outputs": [
        "dist/**/*"
      ]
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY"
  ]
}
