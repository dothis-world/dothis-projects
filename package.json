{
  "name": "dothis-projects",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.14.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@dothis/config": "workspace:*",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0",
    "typescript": "^4.8.4"
  }
}