{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=development nest build --webpack && tsc-alias -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js",
    "start:dev-api": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "clean": "rm -rf .turbo node_modules"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^1.9.2",
    "@googleapis/youtube": "^8.0.2",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "9.0.1",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.0.1",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@ts-rest/core": "^3.18.0",
    "@ts-rest/nest": "^3.17.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "clean-webpack-plugin": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "googleapis": "^112.0.0",
    "ioredis": "^5.3.1",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "run-script-webpack-plugin": "^0.1.1",
    "rxjs": "7.5.5",
    "terser-webpack-plugin": "^5.3.6",
    "tsc-alias": "^1.8.2",
    "typeorm": "^0.3.12",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.1",
    "@types/express": "4.17.13",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/supertest": "2.0.12",
    "@types/webpack-env": "^1.18.0",
    "eslint": "8.19.0",
    "express": "^4.18.2",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.5",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}