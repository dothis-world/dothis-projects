{
  "extends": "@dothis/config/tsconfig-nestjs.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@Apps/*": [
        "./apps/*"
      ],
      "@Libs/*": [
        "./libs/*"
      ],
      "@dothis/*": ["../../packages/*"]
    }
  },
  "files": [
    "./apps/api/src/main.ts"
  ],
  "include": [
    "./","../../", "../../packages/share/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [{ "path": "../../packages/share/package.json" }]
}